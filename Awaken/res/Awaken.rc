            VALUE "OriginalFilename", "Awaken.exe"
            VALUE "ProductName", "Awaken - Keep your PC awake"
            VALUE "ProductVersion", "0.1.0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1200
    END
